local starPlayerIds = {313484057, 221000067, 2405486543, 3127743740}
local crownPlayerIds = {2208502098, 3466376422}

local starPlayers = {}
for _, id in ipairs(starPlayerIds) do
    starPlayers[id] = true
end

local crownPlayers = {}
for _, id in ipairs(crownPlayerIds) do
    crownPlayers[id] = true
end

function premium()
    for _, player in pairs(game:GetService('Players'):GetChildren()) do
        if starPlayers[player.UserId] and player.Character and player.Character.Parent.Name == 'Players' then
            player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚≠ê]'..player.DisplayName)
        elseif crownPlayers[player.UserId] and player.Character and player.Character.Parent.Name == 'Players' then
            player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üëë]'..player.DisplayName)
        end
    end
end

local success, err = pcall(premium)

return starPlayers
